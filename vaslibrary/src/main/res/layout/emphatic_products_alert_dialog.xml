    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.varanegar.framework.util.recycler.BaseRecyclerView
        android:id="@+id/emphatic_packages_recycler_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.varanegar.framework.util.recycler.BaseRecyclerView
        android:id="@+id/emphatic_items_recycler_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>